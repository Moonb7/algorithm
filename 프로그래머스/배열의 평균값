using System;

public class Solution {
    public float solution(int[] numbers) {
        float answer = 0;
        
        for(int i= 0;i <numbers.Length; i++)
        {
            answer += numbers[i];
        }
        float avg = answer / numbers.Length;
        
        return avg;
    }
}

JavaScript

function solution(numbers) {
    let answer = 0;
    let avg;
    for(let i = 0; i < numbers.length; i++)
    {
        answer += numbers[i];
    }
    avg = answer / numbers.length;
    return avg;
}
