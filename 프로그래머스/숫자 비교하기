using System;

public class Solution {
    public int solution(int num1, int num2) {
        int result = 0;
        result = num1 == num2 ? 1 : -1;
        return result;
    }
}

JavaScript
function solution(num1, num2) {
    let answer = 0;
    answer = num1 == num2 ? 1 : -1;
    return answer;
}
const solution = (num1,num2) => {return num1 == num2 ? 1:-1};
